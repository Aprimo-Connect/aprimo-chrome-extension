 {
    "name": "Aprimo",
    "version": "0.1",
    "description": "Aprimo Chrome Extension",
    "permissions": ["declarativeContent", "storage","alarms","notifications","https://*.aprimo.com/","https://*.dam.aprimo.com/"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "login.html",
      "default_title" : "Aprimo DAM Connector",
      "default_icon": {
        "16": "images/a16.png",
        "32": "images/a32.png",
        "48": "images/a48.png",
        "128": "images/a128.png"
      }
    },
    "icons": {
      "16": "images/a16.png",
      "32": "images/a32.png",
      "48": "images/a48.png",
      "128": "images/a128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
  }